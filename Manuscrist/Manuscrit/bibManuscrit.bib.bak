@ARTICLE{Gunning2013,
  author = {C. E. Gunning and H. J. Wearing},
  title = {Probabilistic measures of persistence and extinction in measles (meta)populations.},
  journal = {Ecol Lett},
  year = {2013},
  volume = {16},
  pages = {985--994},
  number = {8},
  month = {Aug},
  abstract = {Persistence and extinction are fundamental processes in ecological
	systems that are difficult to accurately measure due to stochasticity
	and incomplete observation. Moreover, these processes operate on
	multiple scales, from individual populations to metapopulations.
	Here, we examine an extensive new data set of measles case reports
	and associated demographics in pre-vaccine era US cities, alongside
	a classic England & Wales data set. We first infer the per-population
	quasi-continuous distribution of log incidence. We then use stochastic,
	spatially implicit metapopulation models to explore the frequency
	of rescue events and apparent extinctions. We show that, unlike critical
	community size, the inferred distributions account for observational
	processes, allowing direct comparisons between metapopulations. The
	inferred distributions scale with population size. We use these scalings
	to estimate extinction boundary probabilities. We compare these predictions
	with measurements in individual populations and random aggregates
	of populations, highlighting the importance of medium-sized populations
	in metapopulation persistence.},
  doi = {10.1111/ele.12124},
  file = {:/home/tran/Documents/Giang_Document/These/thesegiang/Articles_Jabref/CCS(critical community size)/Gunning Wearing 2013 Probabilistic measures of persistence and extinction in measles.pdf:PDF},
  institution = {Department of Biology, University of New Mexico, Albuquerque, NM,
	USA. xian@unm.edu},
  keywords = {England, epidemiology; Extinction, Biological; Humans; Linear Models;
	Measles, epidemiology; Models, Theoretical; Morbillivirus, physiology;
	Population Density; Probability; Seasons; Stochastic Processes; Time
	Factors; United States, epidemiology; Wales, epidemiology},
  language = {eng},
  medline-pst = {ppublish},
  owner = {tran},
  pmid = {23782847},
  timestamp = {2014.06.11},
  url = {http://dx.doi.org/10.1111/ele.12124}
}

@ARTICLE{Keeling1997,
  author = {M. J. Keeling and B. T. Grenfell},
  title = {Disease extinction and community size: modeling the persistence of
	measles},
  journal = {Science},
  year = {1997},
  volume = {275},
  pages = {65--67},
  number = {5296},
  month = {Jan},
  __markedentry = {[tran:6]},
  abstract = {A basic issue in ecology is the relation between extinction and population
	size. One of the clearest manifestations of a population threshold
	for extinction is the critical community size below which infections
	like measles do not persist. The current generation of stochastic
	models overestimates the observed critical community size for measles,
	generating much less persistence of infection than is observed. The
	inclusion of a more biologically realistic model for the duration
	of infection produced a much closer fit to the actual critical community
