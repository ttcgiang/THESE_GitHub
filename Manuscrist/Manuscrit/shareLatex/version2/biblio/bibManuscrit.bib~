@Article{Bolker1996,
  Title                    = {Impact of vaccination on the spatial correlation and persistence of measles dynamics},
  Author                   = {B. M. Bolker and B. T. Grenfell},
  Journal                  = {Proc Natl Acad Sci U S A},
  Year                     = {1996},

  Month                    = {Oct},
  Number                   = {22},
  Pages                    = {12648--12653},
  Volume                   = {93},

  Abstract                 = {The onset of measles vaccination in England and Wales in 1968 coincided with a marked drop in the temporal correlation of epidemic patterns between major cities. We analyze a variety of hypotheses for the mechanisms driving this change. Straightforward stochastic models suggest that the interaction between a lowered susceptible population (and hence increased demographic noise) and nonlinear dynamics is sufficient to cause the observed drop in correlation. The decorrelation of epidemics could potentially lessen the chance of global extinction and so inhibit attempts at measles eradication.},
  Institution              = {Department of Zoology, Cambridge University, United Kingdom.},
  Keywords                 = {Child; Demography; England, epidemiology; History, 20th Century; Humans; Measles, epidemiology; Vaccination, history; Wales, epidemiology},
  Language                 = {eng},
