@Article{Dey2006,
  Title                    = {Stability via asynchrony in Drosophila metapopulations with low migration rates},
  Author                   = {S. Dey and A. Joshi},
  Journal                  = {Science},
  Year                     = {2006},

  Month                    = {Apr},
  Number                   = {5772},
  Pages                    = {434--436},
  Volume                   = {312},

  __markedentry            = {[tran:6]},
  Abstract                 = {Very few experimental studies have examined how migration rate affects metapopulation dynamics and stability. We studied the dynamics of replicate laboratory metapopulations of Drosophila under different migration rates. Low migration stabilized metapopulation dynamics, while promoting unstable subpopulation dynamics, by inducing asynchrony among neighboring subpopulations. High migration synchronized subpopulation dynamics, thereby destabilizing the metapopulations. Contrary to some theoretical predictions, increased migration did not affect average population size. Simulations based on a simple non-species-specific population growth model captured most features of the data, which suggests that our results are generalizable.},
  Doi                      = {10.1126/science.1125317},
